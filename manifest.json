      "data/surrogates.js",
      "src/surrogates.js",
      "src/multiDomainFirstParties.js",
      "src/incognito.js",
      "src/constants.js",
      "src/storage.js",
      "src/utils.js",
      "src/htmlutils.js",
      "src/webrequest.js",
      "src/heuristicblocking.js",
      "src/socialwidgetloader.js",
      "src/migrations.js",
      "src/background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "src/clobbercookie.js",
        "src/clobberlocalstorage.js",
        "src/fingerprinting.js",
        "src/supercookie.js"
      ],
      "matches": [
        "<all_urls>"
